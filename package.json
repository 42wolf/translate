{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public"
        }
      },
      {
        "src": "api/translate.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/translate",
        "dest": "/api/translate.js"
      }
    ]
  }
  